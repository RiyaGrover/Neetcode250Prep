// Hashmap

class Solution {
    public boolean containsDuplicate(int[] nums) {
        Map<Integer, Boolean> map = new HashMap<>();
        for(int i = 0; i < nums.length;i++){
           if(map.getOrDefault(nums[i], false)){
            return true;
           }
           map.put(nums[i], true);
        }
        return false;
        
    }
}


// hashSet

class Solution {
    public boolean containsDuplicate(int[] nums) {
       Set<Integer> set = new HashSet<>();
       for(int num: nums){
        if(!set.add(num)){
            return true;
        }
       }
       return false;
        
    }
}